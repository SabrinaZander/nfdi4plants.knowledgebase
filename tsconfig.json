{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@images/*": [
        "src/assets/images/*"
      ],
      "@docs/*": [
        "src/docs/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}